<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}MOS{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('build/css/app.css') }}">
        {% endblock %}
        <meta name="viewport" content="width=device-width,initial-scale=1.0">
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico')}}">
        {% block canonical_meta %}{% endblock %}
    </head>
    <body>
        {% block menu %}
          {% include 'menu/main.html.twig'%}
        {% endblock %}
        <div class="container">
            {% block body %}{% endblock %}
            {% block pager %}{% endblock %}
        </div>
        {% block javascripts %}
            <script src="{{ asset('build/js/app.js') }}"></script>
        {% endblock %}
    </body>
</html>
